{
  "adaptive_quality": {
    "enabled": true,
    "enable_adaptation": true,
    "cpu_threshold": 0.8,
    "memory_threshold": 0.8,
    "gpu_threshold": 0.8,
    "default_quality": "MEDIUM",
    "adaptation_interval_ms": 1000.0,
    "enable_predictive_scaling": true,
    "adaptation_strategy": "balanced",
    "min_quality_level": "ULTRA_LOW",
    "max_quality_level": "ULTRA_HIGH",
    "resource_monitoring": {
      "enabled": true,
      "monitoring_interval_ms": 500,
      "history_size": 1000,
      "enable_continuous_monitoring": true
    },
    "performance_prediction": {
      "enabled": true,
      "learning_mode": true,
      "use_neural_network": false,
      "training_data_size": 1000,
      "model_update_interval_minutes": 10,
      "prediction_confidence_threshold": 0.7
    },
    "quality_presets": {
      "ULTRA_LOW": {
        "thread_count": 1,
        "enable_gpu": false,
        "confidence_threshold": 0.3,
        "enable_preprocessing": false,
        "max_buffer_size": 256,
        "enable_quantization": true,
        "quantization_level": "INT8"
      },
      "LOW": {
        "thread_count": 2,
        "enable_gpu": false,
        "confidence_threshold": 0.4,
        "enable_preprocessing": true,
        "max_buffer_size": 512,
        "enable_quantization": true,
        "quantization_level": "INT8"
      },
      "MEDIUM": {
        "thread_count": 4,
        "enable_gpu": true,
        "confidence_threshold": 0.5,
        "enable_preprocessing": true,
        "max_buffer_size": 1024,
        "enable_quantization": false,
        "quantization_level": "AUTO"
      },
      "HIGH": {
        "thread_count": 6,
        "enable_gpu": true,
        "confidence_threshold": 0.6,
        "enable_preprocessing": true,
        "max_buffer_size": 2048,
        "enable_quantization": false,
        "quantization_level": "AUTO"
      },
      "ULTRA_HIGH": {
        "thread_count": 8,
        "enable_gpu": true,
        "confidence_threshold": 0.7,
        "enable_preprocessing": true,
        "max_buffer_size": 4096,
        "enable_quantization": false,
        "quantization_level": "FP32"
      }
    },
    "optimization_recommendations": {
      "enabled": true,
      "max_recommendations": 5,
      "min_improvement_threshold": 0.1,
      "max_implementation_cost": 0.8,
      "recommendation_types": [
        "QUALITY_ADJUSTMENT",
        "RESOURCE_ALLOCATION",
        "CONFIGURATION_CHANGE",
        "HARDWARE_UPGRADE",
        "LOAD_BALANCING"
      ]
    },
    "benchmarking": {
      "enabled": true,
      "auto_calibration": true,
      "calibration_interval_hours": 24,
      "benchmark_scenarios": [
        "all_quality_levels",
        "thread_count_variations",
        "gpu_vs_cpu",
        "buffer_size_variations"
      ],
      "test_audio_lengths": [2, 10, 30],
      "stress_test_duration_seconds": 60,
      "max_concurrent_requests": 8
    },
    "logging": {
      "log_adaptations": true,
      "log_predictions": false,
      "log_resource_changes": true,
      "log_performance_data": true,
      "adaptation_log_level": "INFO",
      "performance_log_level": "DEBUG"
    }
  }
}